@prefix : <http://example.org/music#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix ns3:	<http://example.org/music#> .


<http://example.org/music> rdf:type owl:Ontology ;
    rdfs:label "Music Domain Ontology" ;
    rdfs:comment "An ontology for describing music, artists, genres, and their relationships. Includes hooks to DBpedia." .

# -----------------------------------------------------------------
# --- Classes
# -----------------------------------------------------------------

:Artist rdf:type owl:Class ;
    rdfs:label "Artist" ;
    rdfs:comment "A musician, band, or creator of music." ;
    owl:equivalentClass dbo:MusicalArtist .

:Genre rdf:type owl:Class ;
    rdfs:label "Genre" ;
    rdfs:comment "A category of music (e.g., Rock, Pop, Electronic)." ;
    owl:equivalentClass dbo:MusicGenre .

:Album rdf:type owl:Class ;
    rdfs:label "Album" ;
    rdfs:comment "A collection of tracks released by an artist." ;
    owl:equivalentClass dbo:Album .

:Track rdf:type owl:Class ;
    rdfs:label "Track" ;
    rdfs:comment "A single song or piece of music." ;
    owl:equivalentClass dbo:Song .

:Playlist rdf:type owl:Class ;
    rdfs:label "Playlist" ;
    rdfs:comment "A custom collection of tracks." .

:Country rdf:type owl:Class ;
    rdfs:label "Country" ;
    rdfs:comment "A country, typically of origin for an artist." ;
    owl:equivalentClass dbo:Country .

:City rdf:type owl:Class ;
    rdfs:label "City" ;
    rdfs:comment "A city, typically of origin for an artist or a music hub." ;
    owl:equivalentClass dbo:City .

:RecordLabel rdf:type owl:Class ;
    rdfs:label "Record Label" ;
    rdfs:comment "A company that manages music recording and publishing." ;
    owl:equivalentClass dbo:RecordLabel .

# -----------------------------------------------------------------
# --- Artist Subclasses
# -----------------------------------------------------------------

:SoloArtist rdf:type owl:Class ;
    rdfs:label "Solo Artist" ;
    rdfs:comment "A single individual musician or performer." ;
    rdfs:subClassOf :Artist .

:Band rdf:type owl:Class ;
    rdfs:label "Band" ;
    rdfs:comment "A group of musicians who perform together." ;
    rdfs:subClassOf :Artist .

:HipHopArtist rdf:type owl:Class ;
    rdfs:label "Hip Hop Artist" ;
    rdfs:comment "An artist who primarily performs hip hop music." ;
    rdfs:subClassOf :Artist .

:ElectronicArtist rdf:type owl:Class ;
    rdfs:label "Electronic Artist" ;
    rdfs:comment "An artist who primarily performs electronic music." ;
    rdfs:subClassOf :Artist .

:RockArtist rdf:type owl:Class ;
    rdfs:label "Rock Artist" ;
    rdfs:comment "An artist who primarily performs rock music." ;
    rdfs:subClassOf :Artist .

:LatinArtist rdf:type owl:Class ;
    rdfs:label "Latin Artist" ;
    rdfs:comment "An artist who primarily performs Latin music." ;
    rdfs:subClassOf :Artist .

:PopArtist rdf:type owl:Class ;
    rdfs:label "Pop Artist" ;
    rdfs:comment "An artist who primarily performs pop music." ;
    rdfs:subClassOf :Artist .

# -----------------------------------------------------------------
# --- Genre Subclasses
# -----------------------------------------------------------------

:HipHopGenre rdf:type owl:Class ;
    rdfs:label "Hip Hop Genre" ;
    rdfs:comment "A genre of music that is part of hip hop culture." ;
    rdfs:subClassOf :Genre .

:ElectronicGenre rdf:type owl:Class ;
    rdfs:label "Electronic Genre" ;
    rdfs:comment "A genre of music that is primarily electronic." ;
    rdfs:subClassOf :Genre .

:RockGenre rdf:type owl:Class ;
    rdfs:label "Rock Genre" ;
    rdfs:comment "A genre of music that is part of rock music." ;
    rdfs:subClassOf :Genre .

:LatinGenre rdf:type owl:Class ;
    rdfs:label "Latin Genre" ;
    rdfs:comment "A genre of music that originates from Latin America." ;
    rdfs:subClassOf :Genre .

:PopGenre rdf:type owl:Class ;
    rdfs:label "Pop Genre" ;
    rdfs:comment "A genre of music that is popular and mainstream." ;
    rdfs:subClassOf :Genre .

# -----------------------------------------------------------------
# --- Geographic Subclasses
# -----------------------------------------------------------------

:USCity rdf:type owl:Class ;
    rdfs:label "US City" ;
    rdfs:comment "A city located in the United States." ;
    rdfs:subClassOf :City .

:EuropeanCity rdf:type owl:Class ;
    rdfs:label "European City" ;
    rdfs:comment "A city located in Europe." ;
    rdfs:subClassOf :City .

:LatinAmericanCity rdf:type owl:Class ;
    rdfs:label "Latin American City" ;
    rdfs:comment "A city located in Latin America." ;
    rdfs:subClassOf :City .

:MusicHub rdf:type owl:Class ;
    rdfs:label "Music Hub" ;
    rdfs:comment "A city that is known as a major center for music production and industry." ;
    rdfs:subClassOf :City .

# -----------------------------------------------------------------
# --- Object Properties
# -----------------------------------------------------------------

:hasGenre rdf:type owl:ObjectProperty ;
    rdfs:label "has genre" ;
    rdfs:domain [
        rdf:type owl:Class ;
        owl:unionOf ( :Artist :Album :Track )
    ] ;
    rdfs:range :Genre .

:producedBy rdf:type owl:ObjectProperty ;
    rdfs:label "produced by" ;
    rdfs:domain [
        rdf:type owl:Class ;
        owl:unionOf ( :Album :Track )
    ] ;
    rdfs:range :Artist .

:includesTrack rdf:type owl:ObjectProperty ;
    rdfs:label "includes track" ;
    rdfs:domain [
        rdf:type owl:Class ;
        owl:unionOf ( :Album :Playlist )
    ] ;
    rdfs:range :Track .

:collaboratedWith rdf:type owl:ObjectProperty ;
    rdfs:label "collaborated with" ;
    rdf:type owl:SymmetricProperty ;
    rdfs:domain :Artist ;
    rdfs:range :Artist .

:isFromCountry rdf:type owl:ObjectProperty ;
    rdfs:label "is from country" ;
    rdfs:domain :Artist ;
    rdfs:range :Country .

:isFromCity rdf:type owl:ObjectProperty ;
    rdfs:label "is from city" ;
    rdfs:domain :Artist ;
    rdfs:range :City .

:signedTo rdf:type owl:ObjectProperty ;
    rdfs:label "signed to" ;
    rdfs:domain :Artist ;
    rdfs:range :RecordLabel .

:influenced rdf:type owl:ObjectProperty ;
    rdfs:label "influenced" ;
    rdfs:comment "Indicates that one artist influenced another." ;
    rdfs:domain :Artist ;
    rdfs:range :Artist .

# -----------------------------------------------------------------
# --- Data Properties
# -----------------------------------------------------------------

:artistName rdf:type owl:DatatypeProperty ;
    rdfs:label "artist name" ;
    rdfs:domain :Artist ;
    rdfs:range xsd:string .

:genreName rdf:type owl:DatatypeProperty ;
    rdfs:label "genre name" ;
    rdfs:domain :Genre ;
    rdfs:range xsd:string .

:albumTitle rdf:type owl:DatatypeProperty ;
    rdfs:label "album title" ;
    rdfs:domain :Album ;
    rdfs:range xsd:string .

:trackTitle rdf:type owl:DatatypeProperty ;
    rdfs:label "track title" ;
    rdfs:domain :Track ;
    rdfs:range xsd:string .

:releaseYear rdf:type owl:DatatypeProperty ;
    rdfs:label "release year" ;
    rdfs:domain [
        rdf:type owl:Class ;
        owl:unionOf ( :Album :Track )
    ] ;
    rdfs:range xsd:gYear .

:durationInSeconds rdf:type owl:DatatypeProperty ;
    rdfs:label "duration in seconds" ;
    rdfs:domain :Track ;
    rdfs:range xsd:positiveInteger .

:population rdf:type owl:DatatypeProperty ;
    rdfs:label "population" ;
    rdfs:domain :Country ;
    rdfs:range xsd:positiveInteger .

:activeStartYear rdf:type owl:DatatypeProperty ;
    rdfs:label "active start year" ;
    rdfs:domain :Artist ;
    rdfs:range xsd:gYear .

:activeEndYear rdf:type owl:DatatypeProperty ;
    rdfs:label "active end year" ;
    rdfs:domain :Artist ;
    rdfs:range xsd:gYear .

# -----------------------------------------------------------------
# --- Restrictions (Constraints on Classes)
# -----------------------------------------------------------------

:Artist rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :artistName ;
    owl:cardinality 1 ;
    rdfs:comment "An Artist must have exactly one name."
] .

:Track rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :trackTitle ;
    owl:cardinality 1 ;
    rdfs:comment "A Track must have exactly one title."
] .

:Track rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :durationInSeconds ;
    owl:cardinality 1 ;
    rdfs:comment "A Track must have exactly one duration."
] .

:Track rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :hasGenre ;
    owl:someValuesFrom :Genre ;
    rdfs:comment "A Track must have at least one genre."
] .

:Track rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :releaseYear ;
    owl:cardinality 1 ;
    rdfs:comment "A Track must have exactly one release year."
] .

:Album rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :producedBy ;
    owl:someValuesFrom :Artist ;
    rdfs:comment "An Album must be produced by at least one Artist."
] .

:Album rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty :includesTrack ;
    owl:someValuesFrom :Track ;
    rdfs:comment "An Album must include at least one Track."
] .

# -----------------------------------------------------------------
# --- Inference Rules (Automatic Classification)
# -----------------------------------------------------------------

# Artists who have hip hop genres are HipHopArtists
:HipHopArtist owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :Artist
        [
            rdf:type owl:Restriction ;
            owl:onProperty :hasGenre ;
            owl:someValuesFrom :HipHopGenre
        ]
    )
] .

# Artists who have electronic genres are ElectronicArtists
:ElectronicArtist owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :Artist
        [
            rdf:type owl:Restriction ;
            owl:onProperty :hasGenre ;
            owl:someValuesFrom :ElectronicGenre
        ]
    )
] .

# Artists who have rock genres are RockArtists
:RockArtist owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :Artist
        [
            rdf:type owl:Restriction ;
            owl:onProperty :hasGenre ;
            owl:someValuesFrom :RockGenre
        ]
    )
] .

# Artists who have Latin genres are LatinArtists
:LatinArtist owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :Artist
        [
            rdf:type owl:Restriction ;
            owl:onProperty :hasGenre ;
            owl:someValuesFrom :LatinGenre
        ]
    )
] .

# Artists who have pop genres are PopArtists
:PopArtist owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :Artist
        [
            rdf:type owl:Restriction ;
            owl:onProperty :hasGenre ;
            owl:someValuesFrom :PopGenre
        ]
    )
] .

# Cities in the US are USCities
:USCity owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :City
        [
            rdf:type owl:Restriction ;
            owl:onProperty rdfs:label ;
            owl:someValuesFrom [
                rdf:type rdfs:Literal ;
                owl:oneOf (
                    "New York City" "Los Angeles" "Atlanta" "Miami" 
                    "Chicago" "Seattle" "Boston" "Nashville" "Las Vegas"
                    "Detroit" "Philadelphia" "San Francisco" "Austin"
                )
            ]
        ]
    )
] .

# Major music hubs
:MusicHub owl:equivalentClass [
    rdf:type owl:Class ;
    owl:intersectionOf (
        :City
        [
            rdf:type owl:Restriction ;
            owl:onProperty rdfs:label ;
            owl:someValuesFrom [
                rdf:type rdfs:Literal ;
                owl:oneOf (
                    "New York City" "Los Angeles" "Nashville" "London" 
                    "Stockholm" "Miami" "Atlanta" "Seattle" "Berlin"
                )
            ]
        ]
    )
] .
